<!DOCTYPE html>
<html>
<head>
  <title>Catalog</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div id = "sections">
<%= render 'layouts/header', locals: {:@sections => :@sections} %>
</div>
<div class = "col-md-3" id = "navigation">
<%= render 'pages/navigation', locals: {:@section => @section} %>
</div>
<div class = "col-md-9">
<%= yield %>
<div id = "adminModal"></div>
</div>
</body>
</html>
